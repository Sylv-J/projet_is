users(id INT(11) AUTO_INCREMENT PRIMARY KEY, username VARCHAR(255) NOT NULL, pwd VARCHAR(255) NOT NULL, mail VARCHAR(255) NOT NULL, user_group VARCHAR(255) NOT NULL, units_remaining INT(11), current_units VARCHAR(255));
units(id VARCHAR(255) PRIMARY KEY, id_father VARCHAR(255), id_sons VARCHAR(255), data TEXT, level TINYINT(1), mark TINYINT(2), max_mark TINYINT(2), id_corrector INT(11), date_modif DATE);
msg(id INT(11) AUTO_INCREMENT PRIMARY KEY, object VARCHAR(255), body TEXT, date DATETIME);
msg_link(id INT(11) AUTO_INCREMENT PRIMARY KEY, mfrom VARCHAR(255), mto VARCHAR(255), dest VARCHAR(255), id_msg INT(11));

