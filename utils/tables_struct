users(id INT(11) AUTO_INCREMENT PRIMARY KEY, username VARCHAR(255) NOT NULL, pwd VARCHAR(255) NOT NULL, mail VARCHAR(255) NOT NULL, user_group VARCHAR(255) NOT NULL);
units(id INT(11) PRIMARY KEY, id_father INT(11), id_sons SET("0","0"), data TEXT, level TINYINT(1), mark TINYINT(2), max_mark TINYINT(2), id_corrector INT(11), date_modif DATE);
msg(id INT(11) AUTO_INCREMENT PRIMARY KEY, object VARCHAR(255), body TEXT, date DATE);
msg_link(id INT(11) AUTO_INCREMENT PRIMARY KEY, from VARCHAR(255), to VARCHAR(255), dest VARCHAR(255), id_msg INT(11));
